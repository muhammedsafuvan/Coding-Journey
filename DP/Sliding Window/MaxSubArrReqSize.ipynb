    "def solve(arr,n,k):\n",
    "    ans,res = arr[0],arr[0]\n",
    "    s = 0\n",
    "    for i in range(n-k):\n",
    "        res = sum(arr[i:i+k])\n",
    "        ans = max(ans,res)\n",
    "    return ans\n",
    "            \n",
    "arr = [-1,-2,3,1,-3,2]\n",
    "n = len(arr)\n",
    "k = 2\n",
    "print(solve(arr,n,k))\n",

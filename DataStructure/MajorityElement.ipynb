    "def major(A):\n",
    "    N= len(A)\n",
    "    A.sort()\n",
    "    print(A)\n",
    "    ans,t = 0,0\n",
    "    for i in range(N-1):\n",
    "        if A[i] ==A[i+1]:\n",
    "            t +=1\n",
    "            ans = max(ans,t)\n",
    "        else:\n",
    "            t = 1\n",
    "    return ans\n",
    "A = [3,1,2,3,3,3]  \n",
    "print(major(A))"

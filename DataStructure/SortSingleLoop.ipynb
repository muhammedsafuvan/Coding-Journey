    "def solve(a):\n",
    "    i = 0\n",
    "    n = len(a)\n",
    "    while i < n-1:\n",
    "        if a[i]>a[i+1]:\n",
    "            a[i],a[i+1]=a[i+1],a[i]\n",
    "        i += 1\n",
    "    return a\n",
    "a = [2,1,3,0]\n",
    "print(solve(a))\n",
    "        "
   ]

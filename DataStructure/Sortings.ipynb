   "source": [
    "def bubble(A,n):\n",
    "    for i in range(n):\n",
    "        for j in range(n-i-1):\n",
    "            \n",
    "            if A[j] > A[j+1]:\n",
    "                A[j],A[j+1] = A[j+1],A[j]\n",
    "                \n",
    "    return A\n",
    "    "
   ]
  },
